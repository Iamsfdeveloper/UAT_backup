<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>AR_Donors</fullName>
        <accessLevel>Edit</accessLevel>
        <label>AR Donors</label>
        <sharedTo>
            <roleAndSubordinates>CEO_AR</roleAndSubordinates>
        </sharedTo>
        <criteriaItems>
            <field>Regional_Office_Code__c</field>
            <operation>equals</operation>
            <value>AR</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>All_Donors</fullName>
        <accessLevel>Edit</accessLevel>
        <label>All  Donors</label>
        <sharedTo>
            <group>All_UK_Regional_Donations_Donors</group>
        </sharedTo>
        <booleanFilter>(1 OR 2) AND 3</booleanFilter>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Donor,Organization</value>
        </criteriaItems>
        <criteriaItems>
            <field>Is_Donor__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Regional_Office_Code__c</field>
            <operation>equals</operation>
            <value>UK</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
