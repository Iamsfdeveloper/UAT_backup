    <rules>
        <fullName>Contact Donation Email Acknowledgment</fullName>
        <actions>
            <name>Contact_Donation_Receipt</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>npsp__Opportunity_AcknowledgmentStatus_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>npsp__Opportunity_Acknowledgment_Date_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Emails an acknowledgement to the Primary Contact for an Opportunity when the Acknowledgement Status is set to Email Acknowledgement Now.</description>
        <formula>TEXT(npsp__Acknowledgment_Status__c) = $Label.npsp__sendacknowledgmentfirestatus &amp;&amp;  npsp__Primary_Contact__r.Email &lt;&gt; NULL &amp;&amp; Text(Donation_Type__c)!=&apos;10N&apos; &amp;&amp; Text(Donation_Type__c)!=&apos;30N&apos; &amp;&amp; Text(Donation_Type__c)!=&apos;10D&apos; &amp;&amp; Text(Regional_Office_Code__c) = &apos;UK&apos;</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Contact Donation Email Acknowledgment 30N%2F10N</fullName>
        <actions>
            <name>Contact_Donation_Receipt_30N_10N_ACK</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>npsp__Opportunity_AcknowledgmentStatus_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>npsp__Opportunity_Acknowledgment_Date_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Emails an acknowledgement to the Primary Contact for an Opportunity when the Acknowledgement Status is set to Email Acknowledgement Now and donation type is of 30/10 Nights.</description>
        <formula>TEXT(npsp__Acknowledgment_Status__c) = $Label.npsp__sendacknowledgmentfirestatus &amp;&amp;  npsp__Primary_Contact__r.Email &lt;&gt; NULL &amp;&amp; OR(Text(Donation_Type__c)=&apos;10N&apos;,Text(Donation_Type__c)=&apos;30N&apos;,Text(Donation_Type__c)=&apos;10D&apos;) &amp;&amp; Text(Regional_Office_Code__c) = &apos;UK&apos;</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>ES 30%2FN%2F10N%2F10D Donation Receipt Workflow Rule</fullName>
        <actions>
            <name>ES_30N_10N_10D_Donation_Receipt</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>ES Donation Receipt for 30N/10N/10D</description>
        <formula>AND(
	npsp__Primary_Contact__r.Email &lt;&gt; null,
    ISPICKVAL(StageName, &apos;Setup Completed&apos;), 
    OR(Text(Donation_Type__c)=&apos;10N&apos;,Text(Donation_Type__c)=&apos;30N&apos;,Text(Donation_Type__c)=&apos;10D&apos;),
    OR(ISNEW(),ISCHANGED(StageName)),
    AND(Text(Regional_Office_Code__c)=&apos;ES&apos;)
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
