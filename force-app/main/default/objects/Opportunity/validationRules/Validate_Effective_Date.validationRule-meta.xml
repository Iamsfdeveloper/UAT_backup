<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_Effective_Date</fullName>
    <active>false</active>
    <errorConditionFormula>AND(Effective_Date__c &lt;=TODAY(),OR(ISNEW(),ISCHANGED(Effective_Date__c)))</errorConditionFormula>
    <errorMessage>Effective Date Should be a Future Date</errorMessage>
</ValidationRule>
