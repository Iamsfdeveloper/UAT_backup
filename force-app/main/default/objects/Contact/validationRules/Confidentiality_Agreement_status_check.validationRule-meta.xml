<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Confidentiality_Agreement_status_check</fullName>
    <active>false</active>
    <description>If Confidentiality Agreement is checked then Confidentiality Agreement date will be required field.</description>
    <errorConditionFormula>AND ( Confidentiality_Agreement__c = TRUE , ISBLANK( Confidentiality_Agreement_Date__c ))</errorConditionFormula>
    <errorDisplayField>Confidentiality_Agreement_Date__c</errorDisplayField>
    <errorMessage>Please Enter Confidentiality Agreement Date</errorMessage>
</ValidationRule>
