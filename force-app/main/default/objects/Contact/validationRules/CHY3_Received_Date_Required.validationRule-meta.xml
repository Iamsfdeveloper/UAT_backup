<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CHY3_Received_Date_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(CHY3_Form_Received__c=True,OR(ISNULL(CHY3_Receive_Date__c),ISNULL(CHY3_Expiry_Date__c)))</errorConditionFormula>
    <errorMessage>Please Enter CHY3 Receive Date and CHY3 Expiry Date</errorMessage>
</ValidationRule>
